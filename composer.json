{
    "name": "valb/php.multiple-domains-problem",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "cs-fix": "vendor/bin/php-cs-fixer fix ./src"
    },
    "authors": [
        {
            "name": "valb.mig",
            "email": "valb.mig@gmail.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^3.8",
        "friendsofphp/php-cs-fixer": "^3.75"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "symfony/var-dumper": "^7.3"
    }
}
